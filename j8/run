#!/bin/bash
#0 :0
exec /usr/lib/j8/bin/jconsole "$0" "$@"
)
onfail=:3 :0
  1!:2&2 ARGV
  1!:2&2]13!:12''
  2!:55>:13!:11''
)
9!:27 'onfail 1'
9!:29]1
echo".;:inv 2}.ARGV
exit 0
NB. vim: ft=j
