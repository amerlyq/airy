# vim:ft=sh

alias s.d='systemctl --user disable'
alias s.e='systemctl --user enable'

alias s.a='systemctl --user --all'
alias s.f='systemctl --user --failed'
alias s.g='systemctl --user list-dependencies'
alias s.i='systemctl --user list-unit-files'
alias s.j='systemctl --user list-jobs'
alias s.l='systemctl --user list-units'

alias s.r='systemctl --user start'
alias s.s='systemctl --user status'
alias s.t='systemctl --user stop'

alias s.v='systemctl --user show-environment'
alias s.vi='systemctl --user import-environment'
alias s.vs='systemctl --user set-environment'

alias s.X='loginctl terminate-user $USER'
alias s.B='journalctl -b'
