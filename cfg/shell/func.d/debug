# vim: ft=sh
## <F12> Source on demand when enhancing configs:  source ~/.shell/func.d/debug
printf "
    ~=> rc-reload-*  actualizes last config version
"

alias rc-reload-zsh='source ~/.zshrc && echo "ZSH config reloaded"'
alias rc-reload-dunst='killall dunst; for i in low normal critical; do notify-send --urgency=$i $i; done'
