#!/bin/bash -e
source ~/.shell/func.d/cfgsetup

### LINKS ###
mkdir -p ~/.gnupg && chmod 700 ~/.gnupg
link -t  ~/.gnupg 'gpg.conf' 'gpg-agent.conf'

# link -ct ~/.config/systemd/user 'gpg-agent.service'
# NOTE: Configs for systemd can't be link, only real file.
#       But whole dir ./systemd/ or ./user/ could.
# usvc_activate gpg-agent

echo RELOADAGENT | gpg-connect-agent
