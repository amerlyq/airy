! vim: ft=xdefaults


URxvt*termName:             rxvt-unicode-256color
URxvt*loginShell:           false
URxvt*saveLines:            16192            ! Max=65535
!URxvt*override-redirect:  false

! Turn on/off ISO 14755, *5.2 for keymaps to work (default enabled)
URxvt*iso14755:             false
URxvt*iso14755_52:          false
! Getting the meta key working normally in vim
! The xterm equivalent of URxvt.meta8 is !(vt100.metaSendsEscape)
!URxvt*meta8: true
URxvt*meta8:                false
! URxvt*modifier: meta

URxvt*cutchars:             `"'&()URxvt*,;<=>?@[]{|}$'"`
