#!/bin/bash
# vim: ft=sh
source ~/.bash/functions.d/packages
if [ $? -eq 1 ]; then exit 1; fi
# Main shell apps

LST="$LST mc xsel htop p7zip-full unrar"
LST="$LST rxvt-unicode-256color tmux urlview"
# urxvt | fast terminal, allowing vim-like tabs, show pictures in w3m browser
# tmux  | multi-window customizable terminal with vi navigation, useful for SSH connections
# uget  | download manager, symbiont for flashgot
# xsel  | clipboard tool for rxvt extensions

apt_noprompt $LST
