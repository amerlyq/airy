#!/bin/bash
# vim: ft=sh
source ~/.bash/functions.d/packages
if [ $? -eq 1 ]; then exit 1; fi
# MEDIA

LST="$LST xbindkeys scrot network-manager uget curl cups-pdf lynx elinks sxiv"
# xbindkeys # to XF86AriseVolumeUp and other media keys works
# scrot     # screenshoter
# tixati    #NOT FOUND # nice dark torrent-downloader
# cups-pdf  # virtual pdf printer
# sxiv      # Image-thumbnail viewer, vim-like, ALT OLD: xzgv
LST="$LST fbreader"

apt_noprompt $LST
