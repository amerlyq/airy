#!/bin/bash

# source ~/.bash_export
# if [ $? -ne 0 ]; then exit 1; fi

# To launch shell in certain directory
# ssh -t example.com "cd /foo/bar; exec \$SHELL -l"
# { SS=autossh && hash $SS >/dev/null 2>&1; } || SS=ssh

PRG="$1"
shift
case "$PRG" in
    mpd) ~/.bin/ssht.d/sir-mpd $@ ;;
    srk) ssh srk -t zsh $@ ;;
    *)   ssh $PRG $@ ;;
esac
